{
  "name": "express-generator-node-typescript-apollo-graphql-mongo-cli",
  "description": "Express Generator CLI for Node with Typescript, Apollo GraphQL Server and MongoDB",
  "version": "1.0.6",
  "main": "dist/index.js",
  "preferGlobal": true,
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc && shx rm -rf dist/template && shx cp -r template dist"
  },
  "bin": {
    "express-generator-ntagm": "./dist/index.js"
  },
  "keywords": [
    "Express generator", "apollo server", 
    "graphql", "node", "nodejs", "typescript", 
    "mongo", "mongodb", "mongoose"
  ],
  "git repository": "https://github.com/fatihturker/express-generator-node-typescript-apollo-graphql-mongo-cli",
  "author": "Fatih Turker",
  "license": "MIT",
  "devDependencies": {
    "@types/ejs": "^2.6.0",
    "@types/inquirer": "^0.0.43",
    "@types/node": "^10.9.4",
    "@types/shelljs": "^0.8.0",
    "@types/yargs": "^11.1.1",
    "shx": "^0.3.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "inquirer": "^6.2.0",
    "shelljs": "^0.8.2",
    "yargs": "^12.0.1"
  }
}
